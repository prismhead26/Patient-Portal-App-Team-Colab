<div class="sign-up-cont text-white signup-wrapper">
  <div class="form-box">
    <h2>Signup</h2>
    <form class="form signup-form">
      <div class="form-group input-box">
        <span class="icon"><ion-icon name="person-outline"></ion-icon></span>
        <input class="form-input" type="text" id="name-signup" />
        <label for="name-signup">name:</label>
      </div>
      <div class="form-group input-box">
        <span class="icon"><ion-icon name="mail"></ion-icon></span>
        <input class="form-input" type="text" id="username-signup" />
        <label for="username-signup">username:</label>
      </div>
      <div class="form-group input-box">
        <span class="icon"><ion-icon name="lock-closed"></ion-icon></span>
        <input class="form-input" type="password" id="password-signup" />
        <label for="password-signup">password:</label>
      </div>
      <div class="form-group input-box">
        <span class="icon"><ion-icon name="medical-outline"></ion-icon></span>
        <input class="form-input" type="text" id="clinic-signup" />
        <label for="clinic-signup">clinic:</label>
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">signup</button>
      </div>
    </form>
  </div>
</div> 


<script src="/js/signup.js"></script>